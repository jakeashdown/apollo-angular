{
  "name": "apollo-angular-link-headers",
  "description": "An Apollo Link to easily transform headers from being a key-value object to an instance of HttpHeaders. Great combination with apollo-angular-link-http.",
  "author": "Kamil Kisiela <kamil.kisiela@gmail.com> (http://github.com/kamilkisiela/)",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/bundles/ngApolloLinkHeaders.umd.js",
  "module": "build/fesm5/ngApolloLinkHeaders.js",
  "typings": "build/ngApolloLinkHeaders.d.ts",
  "version": "1.5.0-alpha.0",
  "repository": "https://github.com/apollographql/apollo-angular/tree/master/packages/apollo-angular-link-headers",
  "keywords": [
    "apollo",
    "graphql",
    "angular"
  ],
  "scripts": {
    "build": "ng-packagr -p ng-package.js",
    "test": "yarn test-only",
    "test-only": "jest --config jest.config.js",
    "deploy": "yarn build && npm publish build",
    "deploy:next": "yarn build && npm publish build --tag next"
  },
  "dependencies": {
    "tslib": "^1.11.0"
  },
  "peerDependencies": {
    "@angular/common": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "apollo-link": "^1.0.0"
  },
  "devDependencies": {
    "@angular/common": "9.1.0",
    "@angular/core": "9.1.0",
    "@types/jest": "25.1.4",
    "apollo-link": "1.2.13",
    "graphql": "14.6.0",
    "graphql-tag": "2.10.3",
    "jest": "25.2.3",
    "ng-packagr": "9.1.0",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  }
}
